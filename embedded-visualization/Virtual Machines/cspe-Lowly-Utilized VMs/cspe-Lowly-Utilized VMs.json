        {
          "format": "table",
          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "select\n  e.name as \"VM Name\",\n  e.oid as \"VM OID\",\n  max(mvcpu.utilization)*100 as \"Max vCPU Utilization (%)\",\n  max(mvmem.utilization)*100 as \"Max vMem Utilization (%)\"\nfrom metric mvcpu\njoin metric mvmem on mvcpu.entity_oid = mvmem.entity_oid AND mvmem.time = mvcpu.time\njoin entity e on e.oid = mvmem.entity_oid and e.oid = mvcpu.entity_oid\njoin scope s on s.scoped_oid = e.oid AND s.scoped_type = 'VIRTUAL_MACHINE' AND s.seed_oid IN (${GROUP_OID:sqlstring})\nwhere\n  $__timeFilter(mvcpu.\"time\") AND\n  $__timeFilter(mvmem.\"time\") AND\n  mvcpu.\"entity_type\" = 'VIRTUAL_MACHINE' AND\n  mvmem.\"entity_type\" = 'VIRTUAL_MACHINE' AND\n  mvcpu.\"type\" = 'VCPU' AND\n  mvmem.\"type\" = 'VMEM' AND\n  mvcpu.utilization IS NOT NULL AND\n  mvmem.utilization IS NOT NULL AND\n  e.name like '%${VM_FILTER}%'\nGROUP BY e.name, e.oid, mvcpu.\"type\", mvmem.\"type\"\nHAVING max(mvcpu.utilization)*100 <= ${MAXIMUM_VCPU} AND max(mvmem.utilization)*100 <= ${MAXIMUM_VMEM}\nORDER BY \"VM Name\"",
          "refId": "A",
          "select": [
            [
              {
                "params": [
                  "savings"
                ],
                "type": "column"
              }
            ]
          ],
          "table": "completed_action",
          "timeColumn": "recommendation_time",
          "timeColumnType": "timestamp",
          "where": [
